<h1>New Project</h1>
<%= form_for(@project) do |f| %>
  <% if @project.errors.any? %>
<!--   Finish building error form -->
  <p>
    <%= f.label :name %> <br/>
    <%= f.text_field :name %>
  </p>

  <p>
    <%= f.label :description %> <br/>
    <%= f.text_field :description %>
  </p>

<%= f.submit %>
<% end %>
